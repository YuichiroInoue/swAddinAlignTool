<Window x:Class="swAddinAlignTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:swAddinAlignTool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="start" HorizontalAlignment="Left" Margin="33,31,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Z align" HorizontalAlignment="Left" Margin="33,77,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="XY align" HorizontalAlignment="Left" Margin="33,129,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <Button Content="center" HorizontalAlignment="Left" Margin="33,179,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_3"/>
        <Button Content="export" HorizontalAlignment="Left" Margin="33,231,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_4"/>
        <Button Content="exit" HorizontalAlignment="Left" Margin="33,280,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_5"/>
        <TextBox x:Name="path" HorizontalAlignment="Left" Height="23" Margin="145,236,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
